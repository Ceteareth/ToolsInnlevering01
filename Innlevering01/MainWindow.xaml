<Window x:Class="Innlevering01.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="SupahDupahLevelEditor" Height="600" Width="800">
    <Grid>
        <DockPanel>
            <Menu DockPanel.Dock="Top">
                <MenuItem Header="_File">
                    <MenuItem x:Name="fileOpen" Header="_Open" Click="fileOpen_Click"/>
                    <MenuItem x:Name="fileClose" Header="_Close"/>
                    <MenuItem x:Name="fileSave" Header="_Save"/>
                    <MenuItem x:Name="fileExit" Header="_Exit"/>
                </MenuItem>
            </Menu>
            <TextBox DockPanel.Dock="Bottom"></TextBox>
            <StackPanel x:Name ="tileMainWrap" DockPanel.Dock="Left" Width="160" Loaded="tileContainer_Loaded" Orientation="Vertical" >

            </StackPanel>
            <TextBox Background="White"></TextBox>
        </DockPanel>
    </Grid>
</Window>
